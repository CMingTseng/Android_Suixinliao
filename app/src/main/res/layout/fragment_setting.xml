<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tencent.qcloud.timchat.ui.SettingFragment">

    <com.tencent.qcloud.timchat.ui.customview.TemplateTitle
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titleText="@string/home_setting_tab" />


    <TextView
        android:id="@+id/myId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="text" />

    <TextView
        android:id="@+id/allow_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="no need by my confirm"
        android:layout_gravity="bottom|center_horizontal"
        android:layout_marginBottom="100dp"
        />

</FrameLayout>
